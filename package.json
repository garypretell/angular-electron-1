{
  "name": "angular-electron",
  "version": "12.0.0",
  "description": "Angular 15 with Electron 22 (Typescript + SASS + Hot Reload)",
  "homepage": "https://github.com/maximegris/angular-electron",
  "author": {
    "name": "Maxime GRIS",
    "email": "maxime.gris@gmail.com"
  },
  "keywords": [
    "angular",
    "angular 15",
    "electron",
    "electron 22",
    "nodejs",
    "typescript",
    "playwright",
    "eslint",
    "sass",
    "windows",
    "mac",
    "linux"
  ],
  "main": "app/main.js",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "init": "npm-run-all -p electron:serve ng:serve",
    "start": "electron-forge start",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve",
    "web:build": "npm run build -- -c web-production",
    "electron": "electron",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:build": "npm run build:prod && electron-builder build --publish=never",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && playwright test -c e2e/playwright.config.ts e2e/",
    "e2e:show-trace": "playwright show-trace e2e/tracing/trace.zip",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . Magnum --overwrite --asar --platform=win32 --arch=x64 --icon=assets/icon.ico --prune=true --out=release-builds",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.2.1",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/language-service": "15.2.0",
    "@angular/material": "^15.2.1",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@auth0/angular-jwt": "^5.1.2",
    "@supabase/supabase-js": "^2.7.1",
    "@electron/remote": "2.0.9",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "@timfish/forge-externals-plugin": "0.2.0",
    "@types/lodash-es": "^4.17.6",
    "angular-svg-icon": "^13.0.0",
    "electron-builder": "23.6.0",
    "electron-reload": "2.0.0-alpha.1",
    "electron-squirrel-startup": "1.0.0",
    "lodash-es": "^4.17.21",
    "rxjs": "7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "15.0.0",
    "@angular-devkit/build-angular": "^15.2.1",
    "@angular-devkit/core": "~15.2.1",
    "@angular-devkit/schematics": "~15.2.1",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.1",
    "@angular/compiler-cli": "^15.2.0",
    "@electron-forge/cli": "6.1.1",
    "@electron-forge/maker-deb": "6.1.1",
    "@electron-forge/maker-rpm": "6.1.1",
    "@electron-forge/maker-squirrel": "6.1.1",
    "@electron-forge/maker-zip": "6.1.1",
    "@ngx-translate/core": "14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "@nrwl/angular": "15.9.2",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/workspace": "15.9.2",
    "@playwright/test": "1.30.0",
    "@schematics/angular": "~15.2.1",
    "@types/jasmine": "4.3.1",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "18.14.0",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "conventional-changelog-cli": "2.2.2",
    "electron": "23.1.0",
    "electron-debug": "3.2.0",
    "electron-reloader": "1.2.3",
    "eslint": "8.34.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsdoc": "39.7.5",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jasmine-core": "4.5.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.1",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "7.3.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "ngrok": "4.3.3",
    "node-polyfill-webpack-plugin": "2.0.1",
    "npm-run-all": "4.1.5",
    "nx": "15.9.2",
    "playwright": "1.30.0",
    "tailwind-scrollbar": "^1.3.1",
    "tailwindcss": "^3.1.6",
    "ts-node": "10.9.1",
    "typescript": "~4.8.4",
    "wait-on": "7.0.1",
    "webdriver-manager": "12.1.8"
  },
  "engines": {
    "node": ">= 14.20.0 || >= 16.13.0 || >= 18.10.0"
  },
  "browserslist": [
    "chrome 110"
  ]
}
